@page
@model Web.Admin.Users.Users


<div>
    <table>
        <tr>
            <th>Points</th>
            <th>Username</th>
            <th></th>
        </tr>
        @foreach (var user in Model.Data.Users)
        {
            <tr>
                <td>@user.DiscmanPoints</td>
                <td>
                    <a href="users/@user.Username" up-layer="new">@user.Username </a>
                </td>
                <td>@user.Emoji</td>
            </tr>
        }
    </table>
</div>